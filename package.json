{
  "name": "ethanify-me",
  "version": "2.0.0",
  "resume_version": "2022.feb.22",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ethan605/ethan605.github.io.git"
  },
  "homepage": "https://ethan605.github.io",
  "scripts": {
    "postinstall": "husky install",
    "preanalyse": "react-app-rewired build",
    "analyse": "source-map-explorer 'build/static/js/*.js'",
    "build": "REACT_APP_GITHUB_SHA=$(git rev-parse --short HEAD) react-app-rewired build",
    "postbuild": "cp build/index.html build/200.html",
    "start": "concurrently npm:dev",
    "dev": "REACT_APP_GITHUB_SHA=$(git rev-parse --short HEAD) react-app-rewired start",
    "lint": "eslint src/**/*.ts*",
    "preserve": "REACT_APP_CANONICAL_URL=https://ethanify.me npm run build",
    "serve": "serve -s build",
    "gen-resumes": "./scripts/latex-to-pdf.sh"
  },
  "dependencies": {
    "@types/node": "^17.0.4",
    "@types/react": "^18.0.8",
    "@types/react-dom": "^18.0.3",
    "@types/react-router-dom": "^5.1.7",
    "@types/styled-components": "^5.1.7",
    "@types/webfontloader": "^1.6.32",
    "@typescript-eslint/eslint-plugin": "^5.8.0",
    "@typescript-eslint/parser": "^5.8.0",
    "buffer": "^6.0.3",
    "eslint-config-prettier": "^8.1.0",
    "eslint-config-react-app": "^7.0.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^7.0.1",
    "md5.js": "^1.3.5",
    "prettier": "^2.2.1",
    "react": "^18.1.0",
    "react-app-rewire-styled-components": "^3.0.2",
    "react-app-rewired": "^2.1.8",
    "react-dom": "^18.1.0",
    "react-feather": "^2.0.8",
    "react-router-dom": "^6.2.1",
    "react-scripts": "^5.0.0",
    "react-to-print": "^2.12.3",
    "serve": "^13.0.2",
    "styled-components": "^5.2.1",
    "styled-normalize": "^8.0.7",
    "typescript": "^4.1.5",
    "webfontloader": "^1.6.28"
  },
  "devDependencies": {
    "concurrently": "^7.0.0",
    "lint-staged": "^12.1.3",
    "source-map-explorer": "^2.5.2"
  },
  "lint-staged": {
    "./**/*.(ts|tsx|json)": [
      "prettier --write"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
