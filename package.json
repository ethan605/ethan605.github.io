{
  "name": "ethanify-me",
  "version": "2.0.0",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ethan605/ethan605.github.io.git"
  },
  "homepage": "https://ethan605.github.io",
  "dependencies": {
    "@types/node": "^14.10.2",
    "@types/react": "^16.9.0",
    "@types/react-dom": "^16.9.0",
    "@types/react-router-dom": "^5.1.5",
    "@types/styled-components": "^5.1.3",
    "@types/webfontloader": "^1.6.32",
    "@typescript-eslint/eslint-plugin": "^4.1.1",
    "@typescript-eslint/parser": "^4.1.1",
    "gh-pages": "^3.1.0",
    "md5.js": "^1.3.5",
    "react": "^16.13.1",
    "react-app-rewire-styled-components": "^3.0.2",
    "react-app-rewired": "^2.1.6",
    "react-dom": "^16.13.1",
    "react-feather": "^2.0.8",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^3.4.3",
    "react-to-print": "^2.10.0",
    "styled-components": "^5.2.0",
    "styled-normalize": "^8.0.7",
    "surge": "^0.21.6",
    "typescript": "^4.0.2",
    "webfontloader": "^1.6.28"
  },
  "devDependencies": {
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-react-app": "^5.2.1",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.6",
    "eslint-plugin-react-hooks": "^4.1.0",
    "husky": "^4.3.0",
    "lint-staged": "^10.3.0",
    "prettier": "^2.1.1",
    "source-map-explorer": "^2.5.0"
  },
  "scripts": {
    "preanalyse": "react-app-rewired build",
    "analyse": "source-map-explorer 'build/static/js/*.js'",
    "build": "REACT_APP_GITHUB_SHA=$(git rev-parse HEAD) react-app-rewired build",
    "postbuild": "cp build/index.html build/200.html",
    "dev": "REACT_APP_GITHUB_SHA=$(git rev-parse HEAD) react-app-rewired start",
    "lint": "eslint src/**/*.ts*",
    "predeploy:gh-pages": "REACT_APP_CANONICAL_URL=https://ethan605.github.io npm run build",
    "deploy:gh-pages": "gh-pages -b master -d build",
    "predeploy:surge": "REACT_APP_CANONICAL_URL=https://ethanify.surge.sh npm run build",
    "deploy:surge": "surge --domain https://ethanify.surge.sh build"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "./**/*.(ts|tsx|json)": [
      "prettier --write"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
